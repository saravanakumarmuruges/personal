   "source": [
    "import pandas as pd\n",
    "\n",
    "def remove_outlier(data, norm, column_name):\n",
    "    mean = data[column_name].mean()\n",
    "    sd = data[column_name].std()\n",
    "    output = data[(data[column_name] <= mean + (norm*sd)) & (data[column_name] >= mean - (norm*sd))]\n",
    "    return output"
   ]
  }
